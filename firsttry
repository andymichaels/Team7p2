def exploreOnly() -> int:
  # return sum of each caf for 100 days
  total = 0
  while i <= 100:
    C1 = random.normalvariate(10, 8)
    total += C1
    i+=1
  while i <= 100:
    C2 = random.normalvariate(15, 6)
    total += C2
    i+=1
  while i <= 100:
    C3 = random.normalvariate(12, 5)
    total += C3
    i+=1
  return total


