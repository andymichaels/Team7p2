import random

def exploitOnly() -> int:
  # generate random happiness values for each caf to find highest out of the first 3 days
  C1 = random.normalvariate(10, 8)
  C2 = random.normalvariate(15, 6)
  C3 = random.normalvariate(12, 5)
  
